C code :

#include<stdio.h>

long long factorial(long long n)
{
   
    if(n==1)
    return 1;
    else 
    return(n*factorial(n-1));
}




int main()
{
    long long n,p;
    scanf("%lld",&n);
    p=factorial(n);
    printf("%lld",p);
    return 0;
}
