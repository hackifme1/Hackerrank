C code :

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   char str[10000];
   int i,t,p;
     scanf("%d",&t);
     getchar();
     while(t--)
     {
         
         scanf("%[^\n]%*c", str); 
         p=strlen(str)-1;
         for(i=0;i<=p;i=i+2)
         {
             printf("%c",str[i]);
         }
        printf(" ");
         for(i=1;i<=p;i=i+2)
         {
             printf("%c",str[i]);
         }
         printf("\n");
     }
    return 0;
}
